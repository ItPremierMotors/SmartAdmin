@{
    ViewData["Title"] = "Sin Acceso";
}

<div class="container-fluid">
    <div class="row justify-content-center mt-5">
        <div class="col-md-6 text-center">
            <i class="fal fa-user-slash fa-4x text-danger mb-3 d-block"></i>
            <h3>Acceso No Disponible</h3>
            <p class="text-muted">
                Su cuenta de usuario no esta vinculada a un registro de tecnico en el sistema.
                Contacte al administrador para que vincule su cuenta.
            </p>
            <a href="@Url.Action("Index", "Home")" class="btn btn-primary">
                <i class="fal fa-arrow-left me-1"></i> Volver al Inicio
            </a>
        </div>
    </div>
</div>
